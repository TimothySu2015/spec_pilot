{
  "name": "@specpilot/http-runner",
  "version": "0.1.0",
  "description": "HTTP execution engine for SpecPilot",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "test": "vitest"
  },
  "dependencies": {
    "axios": "1.6.8",
    "@specpilot/config": "workspace:*",
    "@specpilot/shared": "workspace:*"
  },
  "devDependencies": {
    "nock": "13.4.0"
  }
}